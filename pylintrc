[BASIC]
# Good variable names which should always be accepted, separated by a comma.
good-names=i,
           j,
           k,
           x,
           y,
           _,
           ax,
           df,
           mb

[MESSAGES CONTROL]
disable = C0330, C0326
ignore = E203, E231, E266, E501, W503

[format]
max-line-length = 88
